  <div style="width: 80%;padding: 50px;margin-top: 150px;margin-left: 200px;">
  <h2 style="text-align: center;">Danh sách địa điểm</h2>
  <div style="width: 250px;">
      <%= render partial: 'shared/show_messages', locals: {flash: flash} %>
    </div>
    <div style="float: right; margin-bottom: 20px;">
      <%= link_to "Thêm địa điểm", new_restaurant_path, class: "btn btn-primary",style: "font-size: 13px;margin-left: 0px;margin-right: 10px;" %>
      <%= link_to "Đăng xuất", logout_users_path, class: "btn btn-primary",style: "font-size: 13px;margin-left: 0px;margin-right: 10px;" %>
    </div>
    <div style="clear: both;"></div>
    <div class="row">
      <table width=85%; id="myTable" class="table-hover table-bordered table-striped" style="text-align: center;">
        <thead>
          <tr>
            <th width="25%" ; style="text-align: center;">Tên địa điểm</th>
            <th width="20%" style="text-align: center;">Danh mục</th>
            <th width="20%" style="text-align: center;">Địa chỉ</th>
            <th width="5%" style="text-align: center;">kích hoạt</th>
            <th width="5%" style="text-align: center;">Nổi bật</th>
            <th width="5%" style="text-align: center;">Menu</th>
            <th width="20%" style="text-align: center;">Chức năng</th>
          </tr>
        </thead>
        <tbody>
          <%= render partial: 'rest', collection: @current_user.restaurants.order('status desc') %>
        </tbody>
      </table>
    </div>
  </div> <!-- End .module-body -->
  <script type="text/javascript">
    $(document).ready(function(){
      $('#myTable').DataTable();
    });
  </script>

