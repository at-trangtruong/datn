<div class="templatemo-content-container">
  <div class="templatemo-content-widget white-bg">
    <div class="bottom-spacing">
      <!-- Button -->
    </div>
    <h2 class="text-uppercase margin-bottom-10 text-center">Liên hệ</h2>
      <div class="table-responsive">
      <%= render partial: 'shared/show_messages', locals: {flash: flash} %>
        <table id="myTable" class="table-hover table-bordered table-striped">
          <thead>
            <tr class="text-center">
              <th width="5%">#</th>
              <th width="25%">Họ tên</th>
              <th width="20%">Email</th>
              <th width="20%;">Phone</th>
              <th width="30%;">Nội dung</th>
              <th>Chức nang</th>
            </tr>
          </thead>
          <tbody>
          <%= render partial: 'contact', collection: @contacts %>
          </tbody>
        </table>
      </div>
  </div>
  <script type="text/javascript">
    $(document).ready(function(){
      $('#myTable').DataTable();
    });
  </script>
