<tr>
  <td class="align-center"><%= user_counter + 1 %></td>
  <td><%= user.username %></td>
  <td class="align-center"><%= user.fullname %></td>
  <td class="align-center">
    <img src="<%= user.avatar %>" alt="" style="width: 100px;height: 100px; border-radius: 100px;">
  </td>
  <td align="center">
    <%= link_to "Sửa", edit_admin_user_path(user.id), method: :get %>
    <%= link_to "Xóa", admin_user_path(user.id) , method: :delete, data: {confirm: "Nếu bạn xóa người dùng này, các địa điểm của người này cũng sẽ bị xóa. Bạn hãy chắc chắn rằng mình muốn xóa"} %>
 </td>
</tr>
