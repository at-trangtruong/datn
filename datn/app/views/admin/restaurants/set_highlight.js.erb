<% if @restaurant.highlight == 1 %>
var a = "<img src='/assets/tick-circle.gif' alt='<%=@restaurant.id %>-0'/>"
<% else %>
var a = "<img src='/assets/minus-circle.gif' alt='<%=@restaurant.id %>-1' />"
<% end %>
$("#setFeatured-<%= @restaurant.id %>").empty();
$("#setFeatured-<%= @restaurant.id %>").append(a);
